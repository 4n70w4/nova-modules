{
    "name": "mycmdev/nova-modules",
    "description": "A Laravel Nova Tool helping modularize your application",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "nova",
        "cmdev",
        "module",
        "generator"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.15"
    },
    "autoload": {
        "psr-4": {
            "Cmdev\\NovaModules\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cmdev\\NovaModules\\NovaModulesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
